{
  "name": "@llm-playground/llm-app-e2e",
  "version": "0.0.1",
  "private": true,
  "nx": {
    "implicitDependencies": [
      "@llm-playground/llm-app"
    ],
    "targets": {
      "e2e": {
        "executor": "@nx/jest:jest",
        "outputs": [
          "{projectRoot}/test-output/jest/coverage"
        ],
        "options": {
          "jestConfig": "apps/llm-app-e2e/jest.config.cts",
          "passWithNoTests": true
        },
        "dependsOn": [
          "@llm-playground/llm-app:build",
          "@llm-playground/llm-app:serve"
        ]
      }
    }
  }
}
